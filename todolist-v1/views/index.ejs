<%- include("header") %> 

    <div class="box" id="heading">
        <h1 ><%= listTitle %></h1>
    </div>
    
    <div class="box">       
            <% for(var i=0; i<newTask.length; i++){ %>
                <div class="item">
                    <input type="checkbox" >
                    <li> <%= newTask[i]%></li>
                </div>                
            <% } %>
               
        <form class="item" action="/" method="post">
            <input type="text" name="task" placeholder="New Item" required autofocus autocomplete="off">
            <button type="submit" name="list" value=<%= listTitle %>>+</button>
        </form>
    </div>
    
<%- include("footer") %> 